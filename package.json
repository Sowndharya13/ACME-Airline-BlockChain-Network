{
  "engines": {
    "composer": "^0.20.7"
  },
  "name": "airlinev14",
  "version": "0.0.1",
  "parserOptions": {
    "sourceType": "module"
  },
  "description": "This is airlinev10 version 3 - Implementation of Reward Points",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/airlinev12.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "eslintConfig": {
    "eslintIgnore": [
      "TicketBookLogic.js"
    ]
  },
  "no-useless-rename": [
    "error",
    {
      "ignoreImport": true
    }
  ],
  "author": "sowndharya",
  "email": "sowndharya@redblacktree.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "chai": "latest",
    "chai-as-promised": "latest",
    "composer-admin": "^0.20.8",
    "composer-cli": "^0.20.8",
    "composer-client": "^0.20.8",
    "composer-common": "^0.20.8",
    "composer-connector-embedded": "^0.20.8",
    "composer-cucumber-steps": "^0.20.8",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "mkdirp": "latest",
    "mocha": "latest",
    "nyc": "latest"
  }
}
